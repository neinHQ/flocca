{
    "cleanUrls": true,
    "trailingSlash": true,
    "redirects": [
        {
            "source": "/docs",
            "destination": "/docs/"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://api.flocca.app/$1"
        },
        {
            "source": "/docs/",
            "destination": "/docs/index.html"
        }
    ]
}
